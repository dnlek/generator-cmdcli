{
  "name": "<%= projectName %>",
  "bin": {
    "<%= commandName %>": "bin/<%= commandName %>"
  },
  "keywords": [
    "cmdcli"
  ],
  "scripts": {
    "compile": "babel -d lib/ src/",
    "install": "tabtab install --name <%= commandName %>",
    "prepublish": "npm run compile"
  },
  "dependencies": {
    "babel-polyfill": "^6.7.4",
    "chalk": "^1.0.0",
    "cmdcli": "^0.2.0",
    "colors": "^1.0.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-1": "^6.5.0"
  }
}
